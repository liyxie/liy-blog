#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/MyWenJian/Blogs/rulin-blog/pblog-web/node_modules/.store/markdown-it@11.0.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/MyWenJian/Blogs/rulin-blog/pblog-web/node_modules/.store/markdown-it@11.0.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.store/markdown-it@11.0.1/node_modules/markdown-it/bin/markdown-it.js" "$@"
else
  exec node  "$basedir/../.store/markdown-it@11.0.1/node_modules/markdown-it/bin/markdown-it.js" "$@"
fi
