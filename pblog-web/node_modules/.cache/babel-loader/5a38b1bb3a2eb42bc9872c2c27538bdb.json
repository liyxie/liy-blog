    getAboutContent() {\r\n        const that = this;\r\n        var aboutMe = this.$store.state.blogInfo.webSite.aboutMe;\r\n        this.markdownToHtml(aboutMe);\r\n        this.$nextTick(() => {\r\n          // 添加代码复制功能\r\n          this.clipboard = new Clipboard(\".copy-btn\");\r\n          this.clipboard.on(\"success\", () => {\r\n            this.$toast({ type: \"success\", message: \"复制成功\" });\r\n          });\r\n          // 添加图片预览功能\r\n          const imgList = this.$refs.about.getElementsByTagName(\"img\");\r\n          for (var i = 0; i < imgList.length; i++) {\r\n            this.imgList.push(imgList[i].src);\r\n            imgList[i].addEventListener(\"click\", function(e) {\r\n              that.previewImg(e.target.currentSrc);\r\n            });\r\n          }\r\n        });\r\n      },\r\n      markdownToHtml(data) {\r\n        const MarkdownIt = require(\"markdown-it\");\r\n        const md = new MarkdownIt({\r\n          html: true,\r\n          linkify: true,\r\n          typographer: true,\r\n          highlight: function(str, lang) {\r\n            // 当前时间加随机数生成唯一的id标识\r\n            var d = new Date().getTime();\r\n            if (\r\n              window.performance &&\r\n              typeof window.performance.now === \"function\"\r\n            ) {\r\n              d += performance.now();\r\n            }\r\n            const codeIndex = \"xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx\".replace(\r\n              /[xy]/g,\r\n              function(c) {\r\n                var r = (d + Math.random() * 16) % 16 | 0;\r\n                d = Math.floor(d / 16);\r\n                return (c == \"x\" ? r : (r & 0x3) | 0x8).toString(16);\r\n              }\r\n            );\r\n            // 复制功能主要使用的是 clipboard.js\r\n            let html = `<button class=\"copy-btn iconfont iconfuzhi\" type=\"button\" data-clipboard-action=\"copy\" data-clipboard-target=\"#copy${codeIndex}\"></button>`;\r\n            const linesLength = str.split(/\\n/).length - 1;\r\n            // 生成行号\r\n            let linesNum = \"<span aria-hidden=\\\"true\\\" class=\\\"line-numbers-rows\\\">\";\r\n            for (let index = 0; index < linesLength; index++) {\r\n              linesNum = linesNum + \"<span></span>\";\r\n            }\r\n            linesNum += \"</span>\";\r\n            if (lang && hljs.getLanguage(lang)) {\r\n              // highlight.js 高亮代码\r\n              const preCode = hljs.highlight(lang, str, true).value;\r\n              html = html + preCode;\r\n              if (linesLength) {\r\n                html += \"<b class=\\\"name\\\">\" + lang + \"</b>\";\r\n              }\r\n              // 将代码包裹在 textarea 中，由于防止textarea渲染出现问题，这里将 \"<\" 用 \"<\" 代替，不影响复制功能\r\n              return `<pre class=\"hljs\"><code>${html}</code>${linesNum}</pre><textarea style=\"position: absolute;top: -9999px;left: -9999px;z-index: -9999;\" id=\"copy${codeIndex}\">${str.replace(\r\n                /<\\/textarea>/g,\r\n                \"</textarea>\"\r\n              )}</textarea>`;\r\n            }\r\n          }\r\n        });\r\n        // 将markdown替换为html标签\r\n        this.aboutContent = md.render(data);\r\n      },\r\n      previewImg(img) {\r\n        this.$imagePreview({\r\n          images: this.imgList,\r\n          index: this.imgList.indexOf(img)\r\n        });\r\n      }\r\n    },\r\n    computed: {\r\n      avatar() {\r\n        return this.$store.state.blogInfo.webSite.authorAvatar;\r\n      },\r\n      cover() {\r\n        var cover = \"\";\r\n        this.$store.state.blogInfo.pageList.forEach(item => {\r\n          if (item.pageLabel === \"about\") {\r\n            cover = item.pageCover;\r\n          }\r\n        });\r\n        return \"background: url(\" + cover + \") center center / cover no-repeat\";\r\n      }\r\n    }\r\n  };\r\n</script>\r\n\r\n<style scoped>\r\n    /*  调整首图背景不透明度  by 程序儒  2023年4月11日  */\r\n    .bgShade {\r\n        background-color: rgba(0, 0, 0, 0.15);\r\n    }\r\n    .about-content {\r\n        word-break: break-word;\r\n        line-height: 1.8;\r\n        font-size: 18px;\r\n        text-align: center;\r\n    }\r\n\r\n    .my-wrapper {\r\n        text-align: center;\r\n    }\r\n\r\n    .author-avatar {\r\n        transition: all 0.5s;\r\n    }\r\n\r\n    .author-avatar:hover {\r\n        transform: rotate(360deg);\r\n    }\r\n</style>\r\n\r\n"],"mappings":";AA8BA,OAAAA,SAAA;AACA,OAAAC,IAAA;AACA,OAAAC,OAAA;AAEA;EACAC,UAAA;IACAD;EACA;EACAE,QAAA;IACA,KAAAC,eAAA;EACA;EACAC,UAAA;IACA,KAAAC,SAAA,CAAAC,OAAA;EACA;EACAC,QAAA;IACAC,IAAA;MACAC,IAAA;MACAC,OAAA;IACA;MACAD,IAAA;MACAC,OAAA;IACA;EACA;EACAC,IAAA,WAAAA,CAAA;IACA;MACAC,YAAA;MACAC,GAAA,EAAAC,OAAA,CAAAC,GAAA,CAAAC,eAAA;MACAX,SAAA;MACAY,OAAA;IACA;EACA;EACAC,OAAA;IACAf,gBAAA;MACA,MAAAgB,IAAA;MACA,IAAAC,OAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,QAAA,CAAAC,OAAA,CAAAJ,OAAA;MACA,KAAAK,cAAA,CAAAL,OAAA;MACA,KAAAM,SAAA;QACA;QACA,KAAArB,SAAA,OAAAP,SAAA;QACA,KAAAO,SAAA,CAAAsB,EAAA;UACA,KAAAC,MAAA;YAAAC,IAAA;YAAAC,OAAA;UAAA;QACA;QACA;QACA,MAAAb,OAAA,QAAAc,KAAA,CAAAC,KAAA,CAAAC,oBAAA;QACA,SAAAC,CAAA,MAAAA,CAAA,GAAAjB,OAAA,CAAAkB,MAAA,EAAAD,CAAA;UACA,KAAAjB,OAAA,CAAAmB,IAAA,CAAAnB,OAAA,CAAAiB,CAAA,EAAAG,GAAA;UACApB,OAAA,CAAAiB,CAAA,EAAAI,gBAAA,oBAAAC,CAAA;YACApB,IAAA,CAAAqB,UAAA,CAAAD,CAAA,CAAAE,MAAA,CAAAC,UAAA;UACA;QACA;MACA;IACA;IACAjB,eAAAd,IAAA;MACA,MAAAgC,UAAA,GAAAC,OAAA;MACA,MAAAC,EAAA,OAAAF,UAAA;QACAG,IAAA;QACAC,OAAA;QACAC,WAAA;QACAC,SAAA,WAAAA,CAAAC,GAAA,EAAAC,IAAA;UACA;UACA,IAAAC,CAAA,OAAAC,IAAA,GAAAC,OAAA;UACA,IACAC,MAAA,CAAAC,WAAA,IACA,OAAAD,MAAA,CAAAC,WAAA,CAAAC,GAAA,iBACA;YACAL,CAAA,IAAAI,WAAA,CAAAC,GAAA;UACA;UACA,MAAAC,SAAA,0CAAAC,OAAA,CACA,SACA,UAAAC,CAAA;YACA,IAAAC,CAAA,IAAAT,CAAA,GAAAU,IAAA,CAAAC,MAAA;YACAX,CAAA,GAAAU,IAAA,CAAAE,KAAA,CAAAZ,CAAA;YACA,QAAAQ,CAAA,UAAAC,CAAA,GAAAA,CAAA,cAAAI,QAAA;UACA,CACA;UACA;UACA,IAAAnB,IAAA,yHAAAY,SAAA;UACA,MAAAQ,WAAA,GAAAhB,GAAA,CAAAiB,KAAA,OAAAhC,MAAA;UACA;UACA,IAAAiC,QAAA;UACA,SAAAC,KAAA,MAAAA,KAAA,GAAAH,WAAA,EAAAG,KAAA;YACAD,QAAA,GAAAA,QAAA;UACA;UACAA,QAAA;UACA,IAAAjB,IAAA,IAAApD,I