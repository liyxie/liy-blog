#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/MyWenJian/Blogs/rulin-blog/pblog-web/node_modules/.store/buble@0.15.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/MyWenJian/Blogs/rulin-blog/pblog-web/node_modules/.store/buble@0.15.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../buble@0.15.2/node_modules/buble/bin/buble" "$@"
else
  exec node  "$basedir/../../../../../buble@0.15.2/node_modules/buble/bin/buble" "$@"
fi
